#: of pandas.reset_option:56
msgid "compute.use_bottleneck"
msgstr ""

#: of pandas.reset_option:55 pandas.reset_option:61 pandas.reset_option:129
#: pandas.reset_option:141 pandas.reset_option:147 pandas.reset_option:153
#: pandas.reset_option:332
msgid "bool"
msgstr ""

#: of pandas.reset_option:53
msgid ""
"Use the bottleneck library to accelerate if it is installed, the default "
"is True Valid values: False,True [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:62
msgid "compute.use_numexpr"
msgstr ""

#: of pandas.reset_option:59
msgid ""
"Use the numexpr library to accelerate computation if it is installed, the"
" default is True Valid values: False,True [default: True] [currently: "
"True]"
msgstr ""

#: of pandas.reset_option:67
msgid "display.chop_threshold"
msgstr ""

#: of pandas.reset_option:66
msgid "float or None"
msgstr ""

#: of pandas.reset_option:65
msgid ""
"if set to a float value, all float values smaller then the given "
"threshold will be displayed as exactly 0 by repr and friends. [default: "
"None] [currently: None]"
msgstr ""

#: of pandas.reset_option:71
msgid "display.colheader_justify"
msgstr ""

#: of pandas.reset_option:70
msgid "'left'/'right'"
msgstr ""

#: of pandas.reset_option:70
msgid ""
"Controls the justification of column headers. used by DataFrameFormatter."
" [default: right] [currently: right]"
msgstr ""

#: of pandas.reset_option:74
msgid "display.column_space No description available."
msgstr ""

#: of pandas.reset_option:74
msgid "[default: 12] [currently: 12]"
msgstr ""

#: of pandas.reset_option:78
msgid "display.date_dayfirst"
msgstr ""

#: of pandas.reset_option:77 pandas.reset_option:81 pandas.reset_option:93
#: pandas.reset_option:111 pandas.reset_option:117 pandas.reset_option:159
#: pandas.reset_option:226 pandas.reset_option:231 pandas.reset_option:252
#: pandas.reset_option:258 pandas.reset_option:296 pandas.reset_option:310
#: pandas.reset_option:316 pandas.reset_option:322
msgid "boolean"
msgstr ""

#: of pandas.reset_option:77
msgid ""
"When True, prints and parses dates with the day first, eg 20/01/2005 "
"[default: False] [currently: False]"
msgstr ""

#: of pandas.reset_option:82
msgid "display.date_yearfirst"
msgstr ""

#: of pandas.reset_option:81
msgid ""
"When True, prints and parses dates with the year first, eg 2005/01/20 "
"[default: False] [currently: False]"
msgstr ""

#: of pandas.reset_option:88
msgid "display.encoding"
msgstr ""

#: of pandas.reset_option:87
msgid "str/unicode"
msgstr ""

#: of pandas.reset_option:85
msgid ""
"Defaults to the detected encoding of the console. Specifies the encoding "
"to be used for strings returned by to_string, these are generally strings"
" meant to be displayed on the console. [default: UTF-8] [currently: "
"UTF-8]"
msgstr ""

#: of pandas.reset_option:94
msgid "display.expand_frame_repr"
msgstr ""

#: of pandas.reset_option:91
msgid ""
"Whether to print out the full DataFrame repr for wide DataFrames across "
"multiple lines, `max_columns` is still respected, but the output will "
"wrap-around across multiple \"pages\" if its width exceeds "
"`display.width`. [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:101
msgid "display.float_format"
msgstr ""

#: of pandas.reset_option:100
msgid "callable"
msgstr ""

#: of pandas.reset_option:97
msgid ""
"The callable should accept a floating point number and return a string "
"with the desired format of the number. This is used in some places like "
"SeriesFormatter. See formats.format.EngFormatter for an example. "
"[default: None] [currently: None]"
msgstr ""

#: of pandas.reset_option:106
msgid "display.html.border"
msgstr ""

#: of pandas.reset_option:105 pandas.reset_option:164 pandas.reset_option:177
#: pandas.reset_option:183 pandas.reset_option:188 pandas.reset_option:208
#: pandas.reset_option:235 pandas.reset_option:240 pandas.reset_option:266
#: pandas.reset_option:272
msgid "int"
msgstr ""

#: of pandas.reset_option:104
msgid ""
"A ``border=value`` attribute is inserted in the ``<table>`` tag for the "
"DataFrame HTML repr. [default: 1] [currently: 1]"
msgstr ""

#: of pandas.reset_option:112
msgid "display.html.table_schema"
msgstr ""

#: of pandas.reset_option:109
msgid ""
"Whether to publish a Table Schema representation for frontends that "
"support it. (default: False) [default: False] [currently: False]"
msgstr ""

#: of pandas.reset_option:118
msgid "display.html.use_mathjax"
msgstr ""

#: of pandas.reset_option:115
msgid ""
"When True, Jupyter notebook will process table contents using MathJax, "
"rendering mathematical expressions enclosed by the dollar symbol. "
"(default: True) [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:124
msgid "display.large_repr"
msgstr ""

#: of pandas.reset_option:123
msgid "'truncate'/'info'"
msgstr ""

#: of pandas.reset_option:121
msgid ""
"For DataFrames exceeding max_rows/max_cols, the repr (and HTML repr) can "
"show a truncated table (the default from 0.13), or switch to the view "
"from df.info() (the behaviour in earlier versions of pandas). [default: "
"truncate] [currently: truncate]"
msgstr ""

#: of pandas.reset_option:130
msgid "display.latex.escape"
msgstr ""

#: of pandas.reset_option:127
msgid ""
"This specifies if the to_latex method of a Dataframe uses escapes special"
" characters. Valid values: False,True [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:136
msgid "display.latex.longtable :bool"
msgstr ""

#: of pandas.reset_option:133
msgid ""
"This specifies if the to_latex method of a Dataframe uses the longtable "
"format. Valid values: False,True [default: False] [currently: False]"
msgstr ""

#: of pandas.reset_option:142
msgid "display.latex.multicolumn"
msgstr ""

#: of pandas.reset_option:139
msgid ""
"This specifies if the to_latex method of a Dataframe uses multicolumns to"
" pretty-print MultiIndex columns. Valid values: False,True [default: "
"True] [currently: True]"
msgstr ""

#: of pandas.reset_option:148
msgid "display.latex.multicolumn_format"
msgstr ""

#: of pandas.reset_option:145
msgid ""
"This specifies if the to_latex method of a Dataframe uses multicolumns to"
" pretty-print MultiIndex columns. Valid values: False,True [default: l] "
"[currently: l]"
msgstr ""

#: of pandas.reset_option:154
msgid "display.latex.multirow"
msgstr ""

#: of pandas.reset_option:151
msgid ""
"This specifies if the to_latex method of a Dataframe uses multirows to "
"pretty-print MultiIndex rows. Valid values: False,True [default: False] "
"[currently: False]"
msgstr ""

#: of pandas.reset_option:160
msgid "display.latex.repr"
msgstr ""

#: of pandas.reset_option:157
msgid ""
"Whether to produce a latex DataFrame representation for jupyter "
"environments that support it. (default: False) [default: False] "
"[currently: False]"
msgstr ""

#: of pandas.reset_option:165
msgid "display.max_categories"
msgstr ""

#: of pandas.reset_option:163
msgid ""
"This sets the maximum number of categories pandas should output when "
"printing out a `Categorical` or a Series of dtype \"category\". [default:"
" 8] [currently: 8]"
msgstr ""

#: of pandas.reset_option:178
msgid "display.max_columns"
msgstr ""

#: of pandas.reset_option:168
msgid ""
"If max_cols is exceeded, switch to truncate view. Depending on "
"`large_repr`, objects are either centrally truncated or printed as a "
"summary view. 'None' value means unlimited."
msgstr ""

#: of pandas.reset_option:172
msgid ""
"In case python/IPython is running in a terminal and `large_repr` equals "
"'truncate' this can be set to 0 and pandas will auto-detect the width of "
"the terminal and print a truncated object which fits the screen width. "
"The IPython notebook, IPython qtconsole, or IDLE do not run in a terminal"
" and hence it is not possible to do correct auto-detection. [default: 0] "
"[currently: 0]"
msgstr ""

#: of pandas.reset_option:184
msgid "display.max_colwidth"
msgstr ""

#: of pandas.reset_option:181
msgid ""
"The maximum width in characters of a column in the repr of a pandas data "
"structure. When the column overflows, a \"...\" placeholder is embedded "
"in the output. [default: 50] [currently: 50]"
msgstr ""

#: of pandas.reset_option:189
msgid "display.max_info_columns"
msgstr ""

#: of pandas.reset_option:187
msgid ""
"max_info_columns is used in DataFrame.info method to decide if per column"
" information will be printed. [default: 100] [currently: 100]"
msgstr ""

#: of pandas.reset_option:196
msgid "display.max_info_rows"
msgstr ""

#: of pandas.reset_option:195 pandas.reset_option:216
msgid "int or None"
msgstr ""

#: of pandas.reset_option:192
msgid ""
"df.info() will usually show null-counts for each column. For large frames"
" this can be quite slow. max_info_rows and max_info_cols limit this null "
"check only to frames with smaller dimensions than specified. [default: "
"1690785] [currently: 1690785]"
msgstr ""

#: of pandas.reset_option:209
msgid "display.max_rows"
msgstr ""

#: of pandas.reset_option:199
msgid ""
"If max_rows is exceeded, switch to truncate view. Depending on "
"`large_repr`, objects are either centrally truncated or printed as a "
"summary view. 'None' value means unlimited."
msgstr ""

#: of pandas.reset_option:203
msgid ""
"In case python/IPython is running in a terminal and `large_repr` equals "
"'truncate' this can be set to 0 and pandas will auto-detect the height of"
" the terminal and print a truncated object which fits the screen height. "
"The IPython notebook, IPython qtconsole, or IDLE do not run in a terminal"
" and hence it is not possible to do correct auto-detection. [default: 60]"
" [currently: 60]"
msgstr ""

#: of pandas.reset_option:217
msgid "display.max_seq_items"
msgstr ""

#: of pandas.reset_option:212
msgid ""
"when pretty-printing a long sequence, no more then `max_seq_items` will "
"be printed. If items are omitted, they will be denoted by the addition of"
" \"...\" to the resulting string."
msgstr ""

#: of pandas.reset_option:216
msgid ""
"If set to None, the number of items to be printed is unlimited. [default:"
" 100] [currently: 100]"
msgstr ""

#: of pandas.reset_option:222
msgid "display.memory_usage"
msgstr ""

#: of pandas.reset_option:221
msgid "bool, string or None"
msgstr ""

#: of pandas.reset_option:220
msgid ""
"This specifies if the memory usage of a DataFrame should be displayed "
"when df.info() is called. Valid values True,False,'deep' [default: True] "
"[currently: True]"
msgstr ""

#: of pandas.reset_option:227
msgid "display.multi_sparse"
msgstr ""

#: of pandas.reset_option:225
msgid ""
"\"sparsify\" MultiIndex display (don't display repeated elements in outer"
" levels within groups) [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:232
msgid "display.notebook_repr_html"
msgstr ""

#: of pandas.reset_option:230
msgid ""
"When True, IPython notebook will use html representation for pandas "
"objects (if it is available). [default: True] [currently: True]"
msgstr ""

#: of pandas.reset_option:236
msgid "display.pprint_nest_depth"
msgstr ""

#: of pandas.reset_option:235
msgid ""
"Controls the number of nested levels to process when pretty-printing "
"[default: 3] [currently: 3]"
msgstr ""

#: of pandas.reset_option:241
msgid "display.precision"
msgstr ""

#: of pandas.reset_option:239
msgid ""
"Floating point output precision (number of significant digits). This is "
"only a suggestion [default: 6] [currently: 6]"
msgstr ""

#: of pandas.reset_option:247
msgid "display.show_dimensions"
msgstr ""

#: of pandas.reset_option:246
msgid "boolean or 'truncate'"
msgstr ""

#: of pandas.reset_option:244
msgid ""
"Whether to print out dimensions at the end of DataFrame repr. If "
"'truncate' is specified, only print out the dimensions if the frame is "
"truncated (e.g. not display all rows and/or columns) [default: truncate] "
"[currently: truncate]"
msgstr ""

#: of pandas.reset_option:253
msgid "display.unicode.ambiguous_as_wide"
msgstr ""

#: of pandas.reset_option:250 pandas.reset_option:256
msgid ""
"Whether to use the Unicode East Asian Width to calculate the display text"
" width. Enabling this may affect to the performance (default: False) "
"[default: False] [currently: False]"
msgstr ""

#: of pandas.reset_option:259
msgid "display.unicode.east_asian_width"
msgstr ""

#: of pandas.reset_option:267
msgid "display.width"
msgstr ""

#: of pandas.reset_option:262
msgid ""
"Width of the display in characters. In case python/IPython is running in "
"a terminal this can be set to None and pandas will correctly auto-detect "
"the width. Note that the IPython notebook, IPython qtconsole, or IDLE do "
"not run in a terminal and hence it is not possible to correctly detect "
"the width. [default: 80] [currently: 80]"
msgstr ""

#: of pandas.reset_option:273
msgid "html.border"
msgstr ""

#: of pandas.reset_option:270
msgid ""
"A ``border=value`` attribute is inserted in the ``<table>`` tag for the "
"DataFrame HTML repr. [default: 1] [currently: 1] (Deprecated, use "
"`display.html.border` instead.)"
msgstr ""

#: of pandas.reset_option:278
msgid "io.excel.xls.writer"
msgstr ""

#: of pandas.reset_option:277 pandas.reset_option:282 pandas.reset_option:287
#: pandas.reset_option:301 pandas.reset_option:306
msgid "string"
msgstr ""

#: of pandas.reset_option:276
msgid ""
"The default Excel writer engine for 'xls' files. Available options: auto,"
" xlwt. [default: auto] [currently: auto]"
msgstr ""

#: of pandas.reset_option:283
msgid "io.excel.xlsm.writer"
msgstr ""

#: of pandas.reset_option:281
msgid ""
"The default Excel writer engine for 'xlsm' files. Available options: "
"auto, openpyxl. [default: auto] [currently: auto]"
msgstr ""

#: of pandas.reset_option:288
msgid "io.excel.xlsx.writer"
msgstr ""

#: of pandas.reset_option:286
msgid ""
"The default Excel writer engine for 'xlsx' files. Available options: "
"auto, openpyxl, xlsxwriter. [default: auto] [currently: auto]"
msgstr ""

#: of pandas.reset_option:293
msgid "io.hdf.default_format"
msgstr ""

#: of pandas.reset_option:292
msgid "format"
msgstr ""

#: of pandas.reset_option:291
msgid ""
"default format writing format, if None, then put will default to 'fixed' "
"and append will default to 'table' [default: None] [currently: None]"
msgstr ""

#: of pandas.reset_option:297
msgid "io.hdf.dropna_table"
msgstr ""

#: of pandas.reset_option:296
msgid ""
"drop ALL nan rows when appending to a table [default: False] [currently: "
"False]"
msgstr ""

#: of pandas.reset_option:302
msgid "io.parquet.engine"
msgstr ""

#: of pandas.reset_option:300
msgid ""
"The default parquet reader/writer engine. Available options: 'auto', "
"'pyarrow', 'fastparquet', the default is 'auto' [default: auto] "
"[currently: auto]"
msgstr ""

#: of pandas.reset_option:307
msgid "mode.chained_assignment"
msgstr ""

#: of pandas.reset_option:305
msgid ""
"Raise an exception, warn, or no action if trying to use chained "
"assignment, The default is warn [default: warn] [currently: warn]"
msgstr ""

#: of pandas.reset_option:311
msgid "mode.sim_interactive"
msgstr ""

#: of pandas.reset_option:310
msgid ""
"Whether to simulate interactive mode for purposes of testing [default: "
"False] [currently: False]"
msgstr ""

#: of pandas.reset_option:317
msgid "mode.use_inf_as_na"
msgstr ""

#: of pandas.reset_option:314
msgid ""
"True means treat None, NaN, INF, -INF as NA (old way), False means None "
"and NaN are null, but INF, -INF are not NA (new way). [default: False] "
"[currently: False]"
msgstr ""

#: of pandas.reset_option:323
msgid "mode.use_inf_as_null"
msgstr ""

#: of pandas.reset_option:320
msgid ""
"use_inf_as_null had been deprecated and will be removed in a future "
"version. Use `use_inf_as_na` instead. [default: False] [currently: False]"
" (Deprecated, use `mode.use_inf_as_na` instead.)"
msgstr ""

#: of pandas.reset_option:333
msgid "plotting.matplotlib.register_converters"
msgstr ""

#: of pandas.reset_option:326
msgid ""
"Whether to register converters with matplotlib's units registry for "
"dates, times, datetimes, and Periods. Toggling to False will remove the "
"converters, restoring any converters that pandas overwrote. [default: "
"True] [currently: True]"
msgstr ""

