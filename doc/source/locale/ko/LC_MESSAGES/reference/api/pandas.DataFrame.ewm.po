msgid "pandas.DataFrame.ewm"
msgstr ""

#: of pandas.DataFrame.ewm:2
msgid "Provides exponential weighted functions."
msgstr ""

#: of pandas.DataFrame.ewm
msgid "Parameters"
msgstr ""

#: of pandas.DataFrame.ewm:10
msgid "**com**"
msgstr ""

#: of pandas.DataFrame.ewm:9 pandas.DataFrame.ewm:13 pandas.DataFrame.ewm:17
#: pandas.DataFrame.ewm:23
msgid "float, optional"
msgstr ""

#: of pandas.DataFrame.ewm:9
msgid ""
"Specify decay in terms of center of mass, :math:`\\alpha = 1 / (1 + "
"com),\\text{ for } com \\geq 0`"
msgstr ""

#: of pandas.DataFrame.ewm:14
msgid "**span**"
msgstr ""

#: of pandas.DataFrame.ewm:13
msgid ""
"Specify decay in terms of span, :math:`\\alpha = 2 / (span + 1),\\text{ "
"for } span \\geq 1`"
msgstr ""

#: of pandas.DataFrame.ewm:18
msgid "**halflife**"
msgstr ""

#: of pandas.DataFrame.ewm:17
msgid ""
"Specify decay in terms of half-life, :math:`\\alpha = 1 - exp(log(0.5) / "
"halflife),\\text{ for } halflife > 0`"
msgstr ""

#: of pandas.DataFrame.ewm:24
msgid "**alpha**"
msgstr ""

#: of pandas.DataFrame.ewm:21
msgid ""
"Specify smoothing factor :math:`\\alpha` directly, :math:`0 < \\alpha "
"\\leq 1`"
msgstr ""

#: of pandas.DataFrame.ewm:28
msgid "**min_periods**"
msgstr ""

#: of pandas.DataFrame.ewm:27
msgid "int, default 0"
msgstr ""

#: of pandas.DataFrame.ewm:27
msgid ""
"Minimum number of observations in window required to have a value "
"(otherwise result is NA)."
msgstr ""

#: of pandas.DataFrame.ewm:32
msgid "**adjust**"
msgstr ""

#: of pandas.DataFrame.ewm:31
msgid "bool, default True"
msgstr ""

#: of pandas.DataFrame.ewm:31
msgid ""
"Divide by decaying adjustment factor in beginning periods to account for "
"imbalance in relative weightings (viewing EWMA as a moving average)"
msgstr ""

#: of pandas.DataFrame.ewm:36
msgid "**ignore_na**"
msgstr ""

#: of pandas.DataFrame.ewm:35
msgid "bool, default False"
msgstr ""

#: of pandas.DataFrame.ewm:35
msgid ""
"Ignore missing values when calculating weights; specify True to reproduce"
" pre-0.15.0 behavior"
msgstr ""

