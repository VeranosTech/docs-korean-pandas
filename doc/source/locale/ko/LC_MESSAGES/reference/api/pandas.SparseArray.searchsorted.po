msgid "left"
msgstr ""

#: of pandas.SparseArray.searchsorted:15
msgid "``self[i-1] < v <= self[i]``"
msgstr ""

#: of pandas.SparseArray.searchsorted:16
msgid "right"
msgstr ""

#: of pandas.SparseArray.searchsorted:16
msgid "``self[i-1] <= v < self[i]``"
msgstr ""

