#: ../../reference/api/pandas.DataFrame.all.rst:2
msgid "pandas.DataFrame.all"
msgstr ""

#: of pandas.DataFrame.all:2
msgid "Return whether all elements are True, potentially over an axis."
msgstr ""

#: of pandas.DataFrame.all:4
msgid ""
"Returns True unless there at least one element within a series or along a"
" Dataframe axis that is False or equivalent (e.g. zero or empty)."
msgstr ""

#: of pandas.DataFrame.all
msgid "Parameters"
msgstr ""

#: of pandas.DataFrame.all:17
msgid "**axis**"
msgstr ""

#: of pandas.DataFrame.all:16
msgid "{0 or 'index', 1 or 'columns', None}, default 0"
msgstr ""

#: of pandas.DataFrame.all:11
msgid "Indicate which axis or axes should be reduced."
msgstr ""

#: of pandas.DataFrame.all:13
msgid ""
"0 / 'index' : reduce the index, return a Series whose index is the "
"original column labels."
msgstr ""

#: of pandas.DataFrame.all:15
msgid ""
"1 / 'columns' : reduce the columns, return a Series whose index is the "
"original index."
msgstr ""

#: of pandas.DataFrame.all:17
msgid "None : reduce all axes, return a scalar."
msgstr ""

#: of pandas.DataFrame.all:21
msgid "**bool_only**"
msgstr ""

#: of pandas.DataFrame.all:20
msgid "bool, default None"
msgstr ""

#: of pandas.DataFrame.all:20
msgid ""
"Include only boolean columns. If None, will attempt to use everything, "
"then use only boolean data. Not implemented for Series."
msgstr ""

#: of pandas.DataFrame.all:27
msgid "**skipna**"
msgstr ""

#: of pandas.DataFrame.all:26
msgid "bool, default True"
msgstr ""

#: of pandas.DataFrame.all:24
msgid ""
"Exclude NA/null values. If the entire row/column is NA and skipna is "
"True, then the result will be True, as for an empty row/column. If skipna"
" is False, then NA are treated as True, because these are not equal to "
"zero."
msgstr ""

#: of pandas.DataFrame.all:31
msgid "**level**"
msgstr ""

#: of pandas.DataFrame.all:30
msgid "int or level name, default None"
msgstr ""

#: of pandas.DataFrame.all:30
msgid ""
"If the axis is a MultiIndex (hierarchical), count along a particular "
"level, collapsing into a Series."
msgstr ""

#: of pandas.DataFrame.all:35
msgid "**\\*\\*kwargs**"
msgstr ""

#: of pandas.DataFrame.all:34
msgid "any, default None"
msgstr ""

#: of pandas.DataFrame.all:34
msgid ""
"Additional keywords have no effect but might be accepted for "
"compatibility with NumPy."
msgstr ""

