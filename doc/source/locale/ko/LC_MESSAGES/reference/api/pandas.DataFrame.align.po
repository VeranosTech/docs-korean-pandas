#: ../../reference/api/pandas.DataFrame.align.rst:2
msgid "pandas.DataFrame.align"
msgstr ""

#: of pandas.DataFrame.align:2
msgid ""
"Align two objects on their axes with the specified join method for each "
"axis Index."
msgstr ""

#: of pandas.DataFrame.align
msgid "Parameters"
msgstr ""

#: of pandas.DataFrame.align:9
msgid "**other**"
msgstr ""

#: of pandas.DataFrame.align:8
msgid "DataFrame or Series"
msgstr ""

#: of pandas.DataFrame.align:12
msgid "**join**"
msgstr ""

#: of pandas.DataFrame.align:11
msgid "{'outer', 'inner', 'left', 'right'}, default 'outer'"
msgstr ""

#: of pandas.DataFrame.align:15
msgid "**axis**"
msgstr ""

#: of pandas.DataFrame.align:14
msgid "allowed axis of the other object, default None"
msgstr ""

#: of pandas.DataFrame.align:15
msgid "Align on index (0), columns (1), or both (None)"
msgstr ""

#: of pandas.DataFrame.align:19
msgid "**level**"
msgstr ""

#: of pandas.DataFrame.align:18
msgid "int or level name, default None"
msgstr ""

#: of pandas.DataFrame.align:18
msgid ""
"Broadcast across a level, matching Index values on the passed MultiIndex "
"level"
msgstr ""

#: of pandas.DataFrame.align:23
msgid "**copy**"
msgstr ""

#: of pandas.DataFrame.align:22
msgid "boolean, default True"
msgstr ""

#: of pandas.DataFrame.align:22
msgid ""
"Always returns new objects. If copy=False and no reindexing is required "
"then original objects are returned."
msgstr ""

#: of pandas.DataFrame.align:27
msgid "**fill_value**"
msgstr ""

#: of pandas.DataFrame.align:26
msgid "scalar, default np.NaN"
msgstr ""

#: of pandas.DataFrame.align:26
msgid ""
"Value to use for missing values. Defaults to NaN, but can be any "
"\"compatible\" value"
msgstr ""

#: of pandas.DataFrame.align:32
msgid "**method**"
msgstr ""

#: of pandas.DataFrame.align:31
msgid "{'backfill', 'bfill', 'pad', 'ffill', None}, default None"
msgstr ""

#: of pandas.DataFrame.align:30
msgid ""
"Method to use for filling holes in reindexed Series pad / ffill: "
"propagate last valid observation forward to next valid backfill / bfill: "
"use NEXT valid observation to fill gap"
msgstr ""

#: of pandas.DataFrame.align:40
msgid "**limit**"
msgstr ""

#: of pandas.DataFrame.align:39
msgid "int, default None"
msgstr ""

#: of pandas.DataFrame.align:35
msgid ""
"If method is specified, this is the maximum number of consecutive NaN "
"values to forward/backward fill. In other words, if there is a gap with "
"more than this number of consecutive NaNs, it will only be partially "
"filled. If method is not specified, this is the maximum number of entries"
" along the entire axis where NaNs will be filled. Must be greater than 0 "
"if not None."
msgstr ""

#: of pandas.DataFrame.align:43
msgid "**fill_axis**"
msgstr ""

#: of pandas.DataFrame.align:42
msgid "{0 or 'index', 1 or 'columns'}, default 0"
msgstr ""

#: of pandas.DataFrame.align:43
msgid "Filling axis, method and limit"
msgstr ""

#: of pandas.DataFrame.align:47
msgid "**broadcast_axis**"
msgstr ""

#: of pandas.DataFrame.align:46
msgid "{0 or 'index', 1 or 'columns'}, default None"
msgstr ""

#: of pandas.DataFrame.align:46
msgid ""
"Broadcast values along this axis, if aligning two objects of different "
"dimensions"
msgstr ""

