msgid "pandas.DataFrame.reindex"
msgstr ""

#: of pandas.DataFrame.reindex:2
msgid ""
"Conform DataFrame to new index with optional filling logic, placing "
"NA/NaN in locations having no value in the previous index. A new object "
"is produced unless the new index is equivalent to the current one and "
"``copy=False``."
msgstr ""

#: of pandas.DataFrame.reindex
msgid "Parameters"
msgstr ""

#: of pandas.DataFrame.reindex:11
msgid "**labels**"
msgstr ""

#: of pandas.DataFrame.reindex:10 pandas.DataFrame.reindex:14
msgid "array-like, optional"
msgstr ""

#: of pandas.DataFrame.reindex:11
msgid "New labels / index to conform the axis specified by 'axis' to."
msgstr ""

#: of pandas.DataFrame.reindex:15
msgid "**index, columns**"
msgstr ""

#: of pandas.DataFrame.reindex:14
msgid ""
"New labels / index to conform to, should be specified using keywords. "
"Preferably an Index object to avoid duplicating data"
msgstr ""

#: of pandas.DataFrame.reindex:19
msgid "**axis**"
msgstr ""

#: of pandas.DataFrame.reindex:18
msgid "int or str, optional"
msgstr ""

#: of pandas.DataFrame.reindex:18
msgid ""
"Axis to target. Can be either the axis name ('index', 'columns') or "
"number (0, 1)."
msgstr ""

#: of pandas.DataFrame.reindex:30
msgid "**method**"
msgstr ""

#: of pandas.DataFrame.reindex:29
msgid "{None, 'backfill'/'bfill', 'pad'/'ffill', 'nearest'}"
msgstr ""

#: of pandas.DataFrame.reindex:22
msgid ""
"Method to use for filling holes in reindexed DataFrame. Please note: this"
" is only applicable to DataFrames/Series with a monotonically "
"increasing/decreasing index."
msgstr ""

#: of pandas.DataFrame.reindex:26
msgid "None (default): don't fill gaps"
msgstr ""

#: of pandas.DataFrame.reindex:27
msgid "pad / ffill: propagate last valid observation forward to next valid"
msgstr ""

#: of pandas.DataFrame.reindex:29
msgid "backfill / bfill: use next valid observation to fill gap"
msgstr ""

#: of pandas.DataFrame.reindex:30
msgid "nearest: use nearest valid observations to fill gap"
msgstr ""

