msgstr ""

#: of pandas.DataFrame.reindex:33
msgid "Return a new object, even if the passed indexes are the same."
msgstr ""

#: of pandas.DataFrame.reindex:37
msgid "**level**"
msgstr ""

#: of pandas.DataFrame.reindex:36
