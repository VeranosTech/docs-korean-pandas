#: ../../reference/api/pandas.Series.resample.rst:2
msgid "pandas.Series.resample"
msgstr ""

#: of pandas.Series.resample:2
msgid "Resample time-series data."
msgstr ""

#: of pandas.Series.resample:4
msgid ""
"Convenience method for frequency conversion and resampling of time "
"series. Object must have a datetime-like index (`DatetimeIndex`, "
"`PeriodIndex`, or `TimedeltaIndex`), or pass datetime-like values to the "
"`on` or `level` keyword."
msgstr ""

#: of pandas.Series.resample
msgid "Parameters"
msgstr ""

#: of pandas.Series.resample:12
msgid "**rule**"
msgstr ""

#: of pandas.Series.resample:11 pandas.Series.resample:18
msgid "str"
msgstr ""

#: of pandas.Series.resample:12
msgid "The offset string or object representing target conversion."
msgstr ""

#: of pandas.Series.resample:19
msgid "**how**"
msgstr ""

#: of pandas.Series.resample:15
msgid "Method for down/re-sampling, default to 'mean' for downsampling."
msgstr ""

#: of pandas.Series.resample:17
msgid ""
"The new syntax is ``.resample(...).mean()``, or "
"``.resample(...).apply(<func>)``"
msgstr ""

#: of pandas.Series.resample:24
msgid "**axis**"
msgstr ""

#: of pandas.Series.resample:23
msgid "{0 or 'index', 1 or 'columns'}, default 0"
msgstr ""

#: of pandas.Series.resample:22
msgid ""
"Which axis to use for up- or down-sampling. For `Series` this will "
"default to 0, i.e. along the rows. Must be `DatetimeIndex`, "
"`TimedeltaIndex` or `PeriodIndex`."
msgstr ""

#: of pandas.Series.resample:31
msgid "**fill_method**"
msgstr ""

#: of pandas.Series.resample:30
msgid "str, default None"
msgstr ""

#: of pandas.Series.resample:27
msgid "Filling method for upsampling."
msgstr ""

#: of pandas.Series.resample:29
msgid ""
"The new syntax is ``.resample(...).<func>()``, e.g. "
"``.resample(...).pad()``"
msgstr ""

#: of pandas.Series.resample:36
msgid "**closed**"
msgstr ""

#: of pandas.Series.resample:35 pandas.Series.resample:40
msgid "{'right', 'left'}, default None"
msgstr ""

#: of pandas.Series.resample:34
msgid ""
"Which side of bin interval is closed. The default is 'left' for all "
"frequency offsets except for 'M', 'A', 'Q', 'BM', 'BA', 'BQ', and 'W' "
"which all have a default of 'right'."
msgstr ""

#: of pandas.Series.resample:41
msgid "**label**"
msgstr ""

#: of pandas.Series.resample:39
msgid ""
"Which bin edge label to label bucket with. The default is 'left' for all "
"frequency offsets except for 'M', 'A', 'Q', 'BM', 'BA', 'BQ', and 'W' "
"which all have a default of 'right'."
msgstr ""

#: of pandas.Series.resample:45
msgid "**convention**"
msgstr ""

#: of pandas.Series.resample:44
msgid "{'start', 'end', 's', 'e'}, default 'start'"
msgstr ""

#: of pandas.Series.resample:44
msgid ""
"For `PeriodIndex` only, controls whether to use the start or end of "
"`rule`."
msgstr ""

#: of pandas.Series.resample:50
msgid "**kind**"
msgstr ""

#: of pandas.Series.resample:49
msgid "{'timestamp', 'period'}, optional, default None"
msgstr ""

#: of pandas.Series.resample:48
msgid ""
"Pass 'timestamp' to convert the resulting index to a `DateTimeIndex` or "
"'period' to convert it to a `PeriodIndex`. By default the input "
"representation is retained."
msgstr ""

#: of pandas.Series.resample:53
msgid "**loffset**"
msgstr ""

#: of pandas.Series.resample:52
msgid "timedelta, default None"
msgstr ""

#: of pandas.Series.resample:53
msgid "Adjust the resampled time labels."
msgstr ""

#: of pandas.Series.resample:58
msgid "**limit**"
msgstr ""

#: of pandas.Series.resample:57
msgid "int, default None"
msgstr ""

#: of pandas.Series.resample:56
msgid "Maximum size gap when reindexing with `fill_method`."
msgstr ""

#: of pandas.Series.resample:63
msgid "**base**"
msgstr ""

#: of pandas.Series.resample:62
msgid "int, default 0"
msgstr ""

#: of pandas.Series.resample:61
msgid ""
"For frequencies that evenly subdivide 1 day, the \"origin\" of the "
"aggregated intervals. For example, for '5min' frequency, base could range"
" from 0 through 4. Defaults to 0."
msgstr ""

#: of pandas.Series.resample:69
msgid "**on**"
msgstr ""

#: of pandas.Series.resample:68
msgid "str, optional"
msgstr ""

#: of pandas.Series.resample:66
msgid ""
"For a DataFrame, column to use instead of index for resampling. Column "
"must be datetime-like."
msgstr ""

#: of pandas.Series.resample:75
msgid "**level**"
msgstr ""

#: of pandas.Series.resample:74
msgid "str or int, optional"
msgstr ""

#: of pandas.Series.resample:72
msgid ""
"For a MultiIndex, level (name or number) to use for resampling. `level` "
"must be datetime-like."
msgstr ""

